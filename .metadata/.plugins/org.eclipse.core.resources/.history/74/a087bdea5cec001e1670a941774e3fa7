		int matriz2[][] = {{1, 2, 3},{4,2,6},{7,8,2}};
		int[][] matrizDaCero = {
				{ 0, 2, 3, 4 },
				{ 4, 5, 6, 5 },
				{ 7, 8, 9 }
		};
		
		//System.out.print(matriz.length);
		System.out.print( String.valueOf( Ejercicio01.resolver(matriz) ) + '\n' );
		
		System.out.print( Ejercicio01.resolver(matriz2) );
		
		System.out.print( String.valueOf( Ejercicio01.resolver(matriz) ) + '\n' );
		
		System.out.print( Ejercicio01.resolver(matriz2) );
		
		
		
	}
	
	private void formatear(boolean resultado)
	{
		System.out.print( String.valueOf( resultado ) + '\n' );
	}

}
