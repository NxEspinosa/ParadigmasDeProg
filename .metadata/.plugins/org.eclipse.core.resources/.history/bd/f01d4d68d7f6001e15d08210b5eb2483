package edu.unlam.matematica;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class VectorTests {

	@Test
	void test() {
		//arrange 
		Vector v1 = new Vector(1, 2, 3);
		Vector v2 = new Vector(4, 5, 6);
		
		//act
		Vector v3 = v1.sumarVectores(v2);
		
		//assert
		assertEquals(5, v3.x);
		assertEquals(7, v3.y);
		assertEquals(9, v3.z);
		
	}

}
