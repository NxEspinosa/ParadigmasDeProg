package unlam.paradigmas.objetos.ej03;

public class Circulo {
	double radio;
	
	public double obtenerRadio() {
		return radio;
	}
	
	private double diametro() {
		return radio * 2;
	}
	
	private double perimetro() {
		return Math.PI * radio;
	}
	
	private double area() {
		return Math.PI * Math.exp(radio);
	}

	
	
	//•radio
	//•diámetro
	//•perímetro
	//•área
}
