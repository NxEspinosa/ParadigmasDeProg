		assertEquals(cubo.obtenerLado(), 4);
	}
	
	@Test
	void testCambiarLado() {
		cubo.cambiarLado(Cubo.obtenerLado(), 2)
	}
		assertEquals(, 4);
	}	
	
	@Test
	void testobtenerVolumen() {
		assertEquals(cubo.obtenerLado(), 4);
	}	
	
	@Test
	void testobtenerAreaCara() {
		assertEquals(cubo.obtenerLado(), 4);
	}	
	
	@Test
	void testcambiarAreaCara() {
		assertEquals(cubo.obtenerLado(), 4);
	}	
	
	@Test
	void testcambiarVolumen() {
		assertEquals(cubo.obtenerLado(), 4);
	}

